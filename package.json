{
  "name": "nativescript-document-picker",
  "version": "1.0.0",
  "description": "Document Picker without using 3rd party libraries",
  "scripts": {
    "tsc": "cp src/filepicker.d.ts plugin && tsc -skipLibCheck",
    "build": "npm i && npm run tsc",
    "tslint": "tslint \"src/**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\"",
    "plugin.tscwatch": "npm run tsc -- -w",
    "demo.ios": "npm i && npm run tsc && cd demo && tns run ios",
    "clean": "rimraf plugin/ios plugin/android plugin/**/*.d.ts plugin/**/*.js plugin/**/*.js.map plugin/node_modules plugin/package-lock.json",
    "ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/farfromrefuge/nativescript-document-picker.git"
  },
  "keywords": [
    "ecosystem:NativeScript",
    "NativeScript"
  ],
  "author": {
    "name": "Martin Guillon",
    "email": "martin@akylas.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/farfromrefuge/nativescript-document-picker/issues"
  },
  "homepage": "https://github.com/farfromrefuge/nativescript-document-picker",
  "devDependencies": {
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "tns-core-modules": "^6.1.1",
    "tns-platform-declarations": "^6.1.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "~3.6.3"
  },
  "dependencies": {
    "nativescript-perms": "^1.0.9"
  }
}
